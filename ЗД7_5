# Задача 1
array = list(map(int, input("Введите массив из 10 целых чисел через пробел: ").split()))
for i in range(len(array) - 1):
    if array[i] < 0 and array[i + 1] < 0:
        print(f"Пара отрицательных чисел: {array[i]}, {array[i + 1]}")

# Задача 2
array = list(map(int, input("Введите массив целых чисел (размером 10): ").split()))
unique_array = list(set(array))
print("Массив без одинаковых элементов:", unique_array)
